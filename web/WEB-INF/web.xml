<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>val</servlet-name>
        <servlet-class>val</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>valuser</servlet-name>
        <servlet-class>valuser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>lo</servlet-name>
        <servlet-class>lo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignupData</servlet-name>
        <servlet-class>SignupData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignupD</servlet-name>
        <servlet-class>SignupD</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>testData</servlet-name>
        <servlet-class>testData</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>val</servlet-name>
        <url-pattern>/val</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>valuser</servlet-name>
        <url-pattern>/valuser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>lo</servlet-name>
        <url-pattern>/lo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignupData</servlet-name>
        <url-pattern>/SignupData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignupD</servlet-name>
        <url-pattern>/SignupD</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>testData</servlet-name>
        <url-pattern>/testData</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            1
        </session-timeout>
    </session-config>
</web-app>
